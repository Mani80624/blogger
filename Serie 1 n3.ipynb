   "source": [
    "## Serie $\\frac{1}{n^3}$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "45c9a237",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Ingresa un número natural: \n",
      "6\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Ingresa un número natural: \\n\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "2631deae",
   "metadata": {},
   "outputs": [],
   "source": [
    "suma = 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "d58f86ce",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1/ 1 + 1/ 8 + 1/ 27 + 1/ 64 + 1/ 125 + 1/ 216 = 1.1902916666666665\n"
     ]
    }
   ],
   "source": [
    "for i in range(1,n+1):\n",
    "    if i < n:\n",
    "        print(\"1/\", i**3, end=\" + \")\n",
    "    elif i == n:\n",
    "        print(\"1/\", i**3, end=\" = \")\n",
    "        \n",
    "    suma += 1/i**3\n",
    "    \n",
    "print(suma)"
   ]
  },
